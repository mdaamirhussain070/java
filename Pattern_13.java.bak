class Pattern_13 
{
	public static void main(String[] args) 
	{
		int i,j,k;
		for(i=1;i<=5;i++)
		{
			for(j=1;j<=i;j++)
			{
				System.out.print(" ");
			}
			for(k=1;k<=11-2*i;k++)
			{
				if(i==1 || k==1 || k==11-2*i)
				{
					System.out.print("*);
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
